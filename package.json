{
    "author": "Betrix",
    "scripts": {
        "build": "tsc",
        "dev": "cross-env NODE_ENV=dev nodemon src/main.ts",
        "start": "node ./build/main.js",
        "start:pm2": "npx pm2 start --name tarkovhelper ./build/main.js",
        "format": "prettier --config .prettierrc \"./{src,test}/**/*.ts\" --write",
